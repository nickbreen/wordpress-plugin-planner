    "templates": {
        "driver": {
            "id": 2494,
            "name": "driver",
            "code": "[before]\n    <ul class=\"drivers\" title=\"Drivers\">\n[/before]\n<li style=\"color: {@colour,contrast_color}; background-color: {@colour}\">{@post_title}</li>\n[after]\n    </ul>\n[/after]\n",
            "type": "template",
            "slug": "driver"
        },
        "plan": {
            "id": 2505,
            "name": "plan",
            "code": "<dl class=\"plan\"\n    data-plan-id=\"{@id}\"\n    data-view-href=\"{@permalink}\"\n    data-edit-href=\"{@id,get_edit_post_link}\"\n    style=\"color: {@driver.colour,contrast_color}; background-color: {@driver.colour};\">\n    <dt class=\"name\">Job</dt>\n    <dd class=\"name\">{@post_title}</dd>\n    <dt class=\"driver\">Driver</dt>\n    <dd class=\"driver\">{@driver}</dd>\n    <dt class=\"date\">Date</dt>\n    <dd class=\"date\">{@plan_date,fmt_date}</dd>\n    <dt class=\"pu_loc\">Location</dt>\n    <dd class=\"pu_loc\" title=\"{@pu_loc,esc_attr}\">{@pu_loc}</dd>\n    <dt class=\"pu_time\">Time</dt>\n    <dd class=\"pu_time\">{@pu_time}</dd>\n    <dt class=\"flight\">Flight</dt>\n    <dd class=\"flight\">{@flight}</dd>\n    <dt class=\"vehicle\">Vehicle</dt>\n    <dd class=\"vehicle\">{@vehicle}</dd>\n    <dt class=\"contact\">Contact Name</dt>\n    <dd class=\"contact\">{@contact_name}</dd>\n    <dt class=\"contact\">Contact Phone</dt>\n    <dd class=\"contact\">{@contact_phone}</dd>\n</dl>\n",
            "type": "template",
            "slug": "plan"
        },
        "passenger": {
            "id": 3549,
            "name": "passenger",
            "code": "[before]\n<table class=\"passengers\">\n    <caption>Passengers</caption>\n    <thead>\n        <tr>\n            <th>Name</th>\n            <th>Gender</th>\n            <th>D.o.B</th>\n            <th title=\"Age at time of tour\">Age</th>\n            <th>Nationality</th>\n            <th>Pick Up</th>\n            <th>Mobile #</th>\n            <th>Address</th>\n        </tr>\n    </thead>\n    <tbody>\n[/before]\n    <tr>\n        <td>{@name}</td>\n        <td>{@gender}</td>\n        <td>{@dob}</td>\n        <td>{@age}</td>\n        <td>{@nationality}</td>\n        <td>{@pick_up}</td>\n        <td>{@mobile}</td>\n        <td>{@address}</td>\n    </tr>\n[after]\n    </tbody>\n</table>\n[/after]\n",
            "type": "template",
            "slug": "passenger"
        }
    }
